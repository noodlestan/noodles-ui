{
  "name": "@noodles-ui/live-solidjs",
  "version": "0.0.1",
  "description": "Minimal app for rendering design system previews",
  "author": "Andre Torgal",
  "license": "MIT",
  "main": "src/system.ts",
  "scripts": {
    "build": "vite build",
    "dev:vite": "vite dev",
    "dev:tsc": "tsc -p tsconfig.json --watch",
    "dev": "concurrently -k -p \"[{name}]\" -n \"dev:tsc\",\"dev:vite\" -c \"cyan.bold\",\"blue.bold\" \"npm run dev:tsc\" \"npm run dev:vite\""
  },
  "dependencies": {
    "@noodles-ui/core-services": "0.0.1",
    "@noodles-ui/core-styled": "0.0.1",
    "@noodles-ui/core-types": "0.0.1",
    "@noodles-ui/core-unstyled": "0.0.1",
    "@solidjs/router": "^0.13.0",
    "solid-js": "^1.8.15",
    "typescript": "5.3.3",
    "vite": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
