{
  "tokens": {
    "base": {
      "global": {
        "--type-heading-base-fontFamily": "Arial, sans-serif",
        "--type-heading-base-fontSize": "24px",
        "--type-heading-base-lineHeight": "1",
        "--type-heading-base-fontWeight": "300",
        "--type-heading-base-letterSpacing": "normal",
        "--type-heading-page-fontSize": "32px",
        "--type-heading-page-lineHeight": "{--type-heading-base-lineHeight}",
        "--type-heading-page-fontWeight": "{--type-heading-base-fontWeight}",
        "--type-heading-page-letterSpacing": "{--type-heading-base-letterSpacing}",
        "--type-heading-section-fontSize": "24px",
        "--type-heading-section-lineHeight": "{--type-heading-base-lineHeight}",
        "--type-heading-section-fontWeight": "{--type-heading-base-fontWeight}",
        "--type-heading-section-letterSpacing": "{--type-heading-base-letterSpacing}",
        "--type-heading-group-fontSize": "18px",
        "--type-heading-group-lineHeight": "{--type-heading-base-lineHeight}",
        "--type-heading-group-fontWeight": "{--type-heading-base-fontWeight}",
        "--type-heading-group-letterSpacing": "{--type-heading-base-letterSpacing}",
        "--type-heading-item-fontSize": "16px",
        "--type-heading-item-lineHeight": "{--type-heading-base-lineHeight}",
        "--type-heading-item-fontWeight": "{--type-heading-base-fontWeight}",
        "--type-heading-item-letterSpacing": "{--type-heading-base-letterSpacing}",
        "--type-text-base-fontFamily": "Georgia, serif",
        "--type-text-base-fontSize": "14px",
        "--type-text-base-lineHeight": "1.5",
        "--type-text-base-fontWeight": "300",
        "--type-text-base-fletterSpacing": "normal",
        "--type-text-large-fontSize": "24px",
        "--type-text-large-lineHeight": "{--type-text-base-lineHeight}",
        "--type-text-large-fontWeight": "{--type-text-base-fontWeight}",
        "--type-text-large-letterSpacing": "{--type-text-base-letterSpacing}",
        "--type-text-medium-fontSize": "18px",
        "--type-text-medium-lineHeight": "{--type-text-base-lineHeight}",
        "--type-text-medium-fontWeight": "{--type-text-base-fontWeight}",
        "--type-text-medium-letterSpacing": "{--type-text-base-letterSpacing}",
        "--type-text-body-fontSize": "16px",
        "--type-text-body-lineHeight": "{--type-text-base-lineHeight}",
        "--type-text-body-fontWeight": "{--type-text-base-fontWeight}",
        "--type-text-body-letterSpacing": "{--type-text-base-letterSpacing}",
        "--type-text-note-fontSize": "14px",
        "--type-text-note-lineHeight": "{--type-text-base-lineHeight}",
        "--type-text-note-fontWeight": "{--type-text-base-fontWeight}",
        "--type-text-note-letterSpacing": "{--type-text-base-letterSpacing}"
      },
      "surfaces": {
        "stage": {
          "--color-content-primary-color": "#eeeeff",
          "--color-content-muted-color": "#ccccdd",
          "--color-content-disabled-color": "#888899",
          "--surface-base-fg": "{--color-content-primary-color}",
          "--surface-base-bg": "#111122"
        },
        "card": {
          "--surface-base-bg": "#000000"
        },
        "invert": {
          "--surface-base-bg": "#000000"
        }
      }
    },
    "light": {
      "global": {},
      "surfaces": {
        "stage": {
          "--color-content-primary-color": "#111122",
          "--color-content-muted-color": "#444455",
          "--color-content-disabled-color": "#888899",
          "--surface-base-bg": "#eeeeff"
        },
        "card": {
          "--surface-base-bg": "#ffffff"
        },
        "invert": {
          "--color-content-primary-color": "#eeeeff",
          "--color-content-muted-color": "#ccccdd",
          "--color-content-disabled-color": "#888899",
          "--surface-base-bg": "#111122"
        }
      }
    }
  }
}
